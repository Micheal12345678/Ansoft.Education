<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/CSS/Common.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/CSS/Master.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/CSS/Control.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.10.2.js")" type="text/javascript"></script>
    

</head>
<body>
    <div id="frame-top">
        @Html.Partial("_TopBar")
    </div>
    <div id="frame-main">
        <div id="frame-main-left">
           @* @Html.Partial("_Menu")*@
             @{ Html.RenderAction("_Menu", "Home");}          
        </div>
        <div id="frame-main-content">
            @RenderBody()
        </div>
    </div>
</body>
</html>
